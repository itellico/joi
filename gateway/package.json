{
  "name": "gateway",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch --ignore '.claude' --ignore 'node_modules' --ignore '*.log' src/server.ts",
    "dev:nowatch": "tsx src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "db:migrate": "tsx src/db/migrate.ts",
    "db:reset": "tsx src/db/reset.ts",
    "contacts:dedupe": "tsx src/contacts/deduplicate-db.ts",
    "typecheck": "tsc --noEmit",
    "dev:autodev": "tsx watch --ignore '.claude' --ignore 'node_modules' --ignore '*.log' src/autodev/worker.ts",
    "dev:autodev:run": "tsx src/autodev/worker.ts",
    "test": "vitest run",
    "lint": "eslint src/",
    "postinstall": "chmod +x ../node_modules/.pnpm/node-pty@*/node_modules/node-pty/prebuilds/darwin-*/spawn-helper 2>/dev/null || chmod +x node_modules/.pnpm/node-pty@*/node_modules/node-pty/prebuilds/darwin-*/spawn-helper 2>/dev/null || true"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@google/genai": "^1.42.0",
    "@livekit/protocol": "^1.44.0",
    "@notionhq/client": "^5.9.0",
    "@slack/socket-mode": "^2.0.5",
    "@slack/web-api": "^7.14.1",
    "@whiskeysockets/baileys": "7.0.0-rc.9",
    "better-sqlite3": "^12.6.2",
    "chokidar": "^5.0.0",
    "cors": "^2.8.5",
    "croner": "^9.0.0",
    "discord.js": "^14.25.1",
    "dotenv": "^16.4.7",
    "express": "^5.0.1",
    "googleapis": "^171.4.0",
    "livekit-server-sdk": "^2.15.0",
    "mem0ai": "^2.2.3",
    "node-pty": "^1.1.0",
    "ollama": "^0.6.3",
    "openai": "^6.22.0",
    "pg": "^8.13.1",
    "qrcode": "^1.5.4",
    "sharp": "^0.33.5",
    "sqlite3": "^5.1.7",
    "telegram": "^2.26.22",
    "ws": "^8.18.0",
    "youtube-transcript": "^1.2.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.12.0",
    "@types/pg": "^8.11.10",
    "@types/qrcode": "^1.5.6",
    "@types/ws": "^8.5.13",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^4.0.18"
  }
}
